exec  --no-startup-id google-chrome
exec  --no-startup-id gnome-terminal
exec  --no-startup-id /usr/local/bin/pstorm
exec  --no-startup-id $HOME/bin/telegram
exec  --no-startup-id /usr/bin/skype
exec  --no-startup-id /usr/bin/thunderbird

# disable non gui menu
bindsym $mod+d exec --no-startup-id i3-dmenu-desktop
xbindkeys -f $HOME/.xbindkeysrc

